int save(struct character_hp *all_hp, struct object *all_obj,int day)
{
	FILE *hp_data = fopen("hp_data.txt", "w");
	FILE *obj_data = fopen("obj_data.txt", "w");
	fprintf(hp_data, "%d %d %d %d %d %d %d %d %d %d", all_hp->a_hp, all_hp->b_hp, all_hp->c_hp,days,a_foodeat,a_watereat,b_foodeat,b_watereat,c_foodeat,c_watereat);
	fprintf(obj_data, "%d %d %d %d ", all_obj->food, all_obj->water, all_obj->healpack, all_obj->gun);
	if (hp_data == NULL || obj_data == NULL)
	{
		puts("저장하기 실패!");
		Sleep(500);
		return -1;
	}
	else
	{
		printf("저장성공");
		Sleep(500);
	}
	fclose(hp_data);
	fclose(obj_data);
	return 0;
}

int load(struct character_hp const *all_hp, struct object const *all_obj, int day)
{
	char ptr_hp[20] = { 0, };
	char ptr_obj[10] = { 0, };
	FILE *hp_data = fopen("hp_data.txt", "r");
	FILE *obj_data = fopen("obj_data.txt", "r");
	for (int i = 0; i<20; i++)
	{
		fscanf(hp_data, "%d", &ptr_hp[i]);
	}
	hp.a_hp = ptr_hp[0];
	hp.b_hp = ptr_hp[1];
	hp.c_hp = ptr_hp[2];
	days = ptr_hp[3];
	a_foodeat = ptr_hp[4];
	a_watereat = ptr_hp[5];
	b_foodeat = ptr_hp[6];
	b_watereat = ptr_hp[7];
	c_foodeat = ptr_hp[8];
	c_watereat = ptr_hp[9];
	for (int i = 0; i < 10; i++)
	{
		fscanf(obj_data, "%d", &ptr_obj[i]);
	}
	obj.food = ptr_obj[0];
	obj.water = ptr_obj[1];
	obj.healpack = ptr_obj[2];
	obj.gun = ptr_obj[3];
}

//utile
void init()
{
	system("mode con cols=120 lines=35 | title 게임 제목");

	HANDLE consoleHandle = GetStdHandle(STD_OUTPUT_HANDLE);
	CONSOLE_CURSOR_INFO ConsoleCursor;
	ConsoleCursor.bVisible = 0;
	ConsoleCursor.dwSize = 1;
	SetConsoleCursorInfo(consoleHandle, &ConsoleCursor);

}

void gotoxy(int x, int y)
{
	HANDLE consoleHandle = GetStdHandle(STD_OUTPUT_HANDLE);
	COORD pos;
	pos.X = x;
	pos.Y = y;
	SetConsoleCursorPosition(consoleHandle, pos);
}

void setColor(int forground, int background)
{
	HANDLE consolHandle = GetStdHandle(STD_OUTPUT_HANDLE);
	int code = forground + background * 16;
	SetConsoleTextAttribute(consolHandle, code);
}
