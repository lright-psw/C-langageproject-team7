//메크로
//키보드 값
#define UP 72
#define DOWN 80
#define LEFT 75
#define RIGHT 77
#define SELECT 13
#define SPACEBAR 32
#define HP 3


int object_num;
int explore_num;
int explore_object;
int explore_q; //정찰 유무
int days = 0;
int days_error = 1;
int explore_now = 1;
int explore_gowithgun = 0;
int who_explore;
extern int random_food = 0;
extern int random_water = 0;
extern int random_healpack = 0;
extern int random_gun = 0;
int random_objnumber_high = 0;
int random_objnumber_middle = 0;
int random_objnumber_low = 0;
int food_select; //식량 선택함수
int water_select;//물 선택함수
int a_foodeat = 0;
int a_watereat = 0;
int b_foodeat = 0;
int b_watereat = 0;
int c_foodeat = 0;
int c_watereat = 0;
int youdied = 1;
int heal_go;
int heal_select;
int heal_who;
int dayback_error = 0;
int special_select;
int special_num;
int nomal_select;
int nomal_num;
int hidden;
int luckygun;

int main()
{
	obj.food = 6;
	obj.water = 6;
	obj.healpack = 1;
	obj.gun = 1;
	hp.a_hp = HP;
	hp.b_hp = HP;
	hp.c_hp = HP;
	luckygun = 0;
	init();
	while (1)
	{
		titledraw();
		int menucode = menudraw();
		if (menucode == 0) //게임시작
		{
			obj.food = 6;
			obj.water = 6;
			obj.healpack = 1;
			obj.gun = 1;
			a_foodeat = 0;
			a_watereat = 0;
			b_foodeat = 0;
			b_watereat = 0;
			c_foodeat = 0;
			c_watereat = 0;
			days = 0;
			youdied = 1;
			hp.a_hp = HP;
			hp.b_hp = HP;
			hp.c_hp = HP;
		rabel:
			luckygun = 0;
			while (1)
			{
				srand((unsigned)time(NULL));
				random_objnumber_high = ((rand() % 4) + 2);
				random_objnumber_middle = ((rand() % 3) + 1);
				random_objnumber_low = ((rand() % 2)+1);
				system("cls");
				if (dayback_error == 0)
				{
					if (days == 0)
					{
						system("cls");
						firstday_epilog();
						Sleep(2500);
					}
					else
						daydraw(days);
				}
				system("cls");
				mapdraw();
				if (hp.a_hp > 0)
				{
					character_a();
				}
				if (hp.b_hp > 0)
				{
					character_b();
				}
				if (hp.c_hp > 0)
				{
					character_c();
				}
				if (days == 0)
				{
					gotoxy(110, 1);
					printf("1번째날");
				}
				obj_amount(obj.food, obj.water, obj.healpack, obj.gun, days);
				selectdraw();
				int select = select_chose();
				if (explore_now != 0)
				{
					explore_now = 1;
				}
				if (select == 0) //정찰
				{
					
					if (explore_now == 0) //정찰 막기!!
					{
						selectdraw();
						gotoxy(1, 24);
						printf("이미 정찰을 보냈습니다.");
						gotoxy(1, 26);
						printf("ENTER키를 눌러주세요......");
						while (1)
						{
							if (keycontrol() == SELECT)
							{
								break;
							}
						}
					}
					while (explore_now == 1)
					{
						selectdraw();
						explore_explain();
						explore_num = human_select();
						if (explore_num == 0) //여자
						{
							selectdraw();
							explore_object = explore_object_select();
							if (explore_object == 0) //총 들고 정찰
							{
								explore_q = 1;
								explore_gowithgun = 1;
								if (obj.gun < 1)
								{
									selectdraw();
									int x = 1;
									int y = 24;
									gotoxy(x, y);
									printf("총이 없습니다. ");
									gotoxy(x, y + 3);
									printf("ENTER키를 눌러주세요......");
									explore_q = 0;
									explore_gowithgun = 0;
									while (1)
									{
										if (keycontrol() == SELECT)
										{
											break;
										}
									}
								}

								if (obj.gun <= 0)
								{
									obj.gun = 0;
									dayback_error = 1;
									who_explore = 1;
								}
								else
								{
									luckygun = 1;
									obj.gun -= 1;
									explore_now = 0;
									dayback_error = 1;
									who_explore = 1;
								}
								days_error = 0;
							}
							else if (explore_object == 30) //총 없이 정찰
							{
								explore_q = 1;
								explore_now = 0;
								days_error = 0;
								dayback_error = 1;
								who_explore = 1;
							}
							else if (explore_object = 60) //뒤로가기
							{
								days_error = 0;
								dayback_error = 1;
							}
						}
						else if (explore_num == 20) //남자
						{
							selectdraw();
							explore_object = explore_object_select();
							if (explore_object == 0) //총 들고 정찰
							{
								explore_q = 1;
								explore_gowithgun = 1;
								if (obj.gun < 1)
								{
									selectdraw();
									int x = 1;
									int y = 24;
									gotoxy(x, y);
									printf("총이 없습니다. ");
									gotoxy(x, y + 3);
									printf("ENTER키를 눌러주세요......");
									explore_q = 0;
									explore_gowithgun = 0;
									while (1)
									{
										if (keycontrol() == SELECT)
										{
											break;
										}
									}
								}
								if (obj.gun <= 0)
								{
									obj.gun = 0;
									dayback_error = 1;
									who_explore = 2;
								}
								else
								{
									luckygun = 1;
									obj.gun -= 1;
									explore_now = 0;
									dayback_error = 1;
									who_explore = 2;
								}
								days_error = 0;
							}
							else if (explore_object == 30) //총 없이 정찰
							{
								explore_q = 1;
								explore_now = 0;
								days_error = 0;
								dayback_error = 1;
								who_explore = 2;
							}
							else if (explore_object = 60) //뒤로가기
							{
								days_error = 0;
								dayback_error = 1;
							}
						}
						else if (explore_num == 40) //아이
						{
							selectdraw();
							explore_object = explore_object_select();
							if (explore_object == 0) //총 들고 정찰
							{
								explore_q = 1;
								explore_gowithgun = 1;
								if (obj.gun < 1)
								{
									selectdraw();
									int x = 1;
									int y = 24;
									gotoxy(x, y);
									printf("총이 없습니다. ");
									gotoxy(x, y + 3);
									printf("ENTER키를 눌러주세요......");
									explore_q = 0;
									explore_gowithgun = 0;
									while (1)
									{
										if (keycontrol() == SELECT)
										{
											break;
										}
									}
								}
								if (obj.gun <= 0)
								{
									obj.gun = 0;
									dayback_error = 1;
									who_explore = 3;
								}
								else
								{
									luckygun = 1;
									obj.gun -= 1;
									explore_now = 0;
									dayback_error = 1;
									who_explore = 3;
								}
								days_error = 0;
							}
							else if (explore_object == 30) //총 없이 정찰
							{
								explore_q = 1;
								explore_now = 0;
								days_error = 0;
								dayback_error = 1;
								who_explore = 3;
							}
							else if (explore_object = 60) //뒤로가기
							{
								days_error = 0;
								dayback_error = 1;
							}
						}
						else if (explore_num == 60) //돌아가기
						{
						explore_now = 2;
						dayback_error = 1;
 }
					}
				}
				else if (select == 20) //캐릭터관리
				{
					selectdraw();
					object_num = object_select();
					if (object_num == 0) //식량
					{
						selectdraw();
						food_select = human_select();
						if (food_select == 0)
						{
							eat_food(obj.food);
							a_foodeat -= 1;
							dayback_error = 1;

						}
						else if (food_select == 20)
						{
							eat_food(obj.food);
							b_foodeat -= 1;
							dayback_error = 1;

						}
						else if (food_select == 40)
						{
							eat_food(obj.food);
							c_foodeat -= 1;
							dayback_error = 1;
						}
						else if (food_select == 60)
						{
							dayback_error = 1;
						}
					}
					else if (object_num == 20) //물
					{
						selectdraw();
						water_select = human_select();
						if (water_select == 0)
						{
							eat_water(obj.food);
							a_watereat -= 1;
							dayback_error = 1;
						}
						else if (water_select == 20)
						{
							eat_water(obj.food);
							b_watereat -= 1;
							dayback_error = 1;
						}
						else if (water_select == 40)
						{
							eat_water(obj.food);
							c_watereat -= 1;
							dayback_error = 1;
						}
						else if (water_select == 60)
						{
							dayback_error = 1;
						}
					}
					else if (object_num == 40) //상태창
					{
						selectdraw();
						statedraw();
						heal_go = healdraw();
						if (heal_go == 0)
						{
							selectdraw();
							heal_select = human_select();
							if (heal_select == 0)
							{
								heal_who = 0;
								heal_hp(&hp);
								dayback_error = 1;
							}
							else if (heal_select == 20)
							{
								heal_who = 1;
								heal_hp(&hp);
								dayback_error = 1;
							}
							else if (heal_select == 40)
							{
								heal_who = 2;
								heal_hp(&hp);
								dayback_error = 1;
							}
							else if (heal_select == 60)
							{
								dayback_error = 1;
							}
						}
						else
							dayback_error = 1;

					}
					else if (object_num == 60) //돌아가기
					{
						dayback_error = 1;
					}

				}
				else if (select == 40) //다음날
				{
					days_error = 1;
					explore_now = 1;
					days += 1;
					a_watereat += 1;
					a_foodeat += 1;
					b_watereat += 1;
					b_foodeat += 1;
					c_watereat += 1;
					c_foodeat += 1;
					dayback_error = 0;
					noeat_down(&hp);
					you_died(&hp);
					if (youdied == 0)
					{
						break;
					}
					if (days == 15 || hidden >= 4)
					{
						hiddenend_epilog();
						gotoxy(19, 29);
						printf("ENTER키를 눌러주세요......");
						while (1)
						{
							if (keycontrol() == SELECT)
							{
								break;
							}
						}
						system("cls");
						gotoxy(25, 10);
						printf("<히든엔딩> 사건의 전말");
						gotoxy(25, 12);
						printf("ENTER키를 눌러주세요......");
						while (1)
						{
							if (keycontrol() == SELECT)
							{
								break;
							}
						}
						break;
					}
					else if (days == 15)
					{
						nomalend_epilog();
						gotoxy(19, 29);
						printf("ENTER키를 눌러주세요......");
						while (1)
						{
							if (keycontrol() == SELECT)
							{
								break;
							}
						}
						system("cls");
						gotoxy(25, 10);
						printf("<노말엔딩> 힘없는 죽음");
						gotoxy(25, 12);
						printf("ENTER키를 눌러주세요......");
						while (1)
						{
							if (keycontrol() == SELECT)
							{
								break;
							}
						}
						break;
					}
				}
				else if (select == 60) //세이브
				{
				save(&hp, &obj, days);
				dayback_error = 1;
 }
			}
				
		}
		else if (menucode == 1) //이어하기
		{
		load(&hp, &obj, days);
		goto rabel;
 }
		else if (menucode == 2) //게임설명
		{
			infodraw();
		}
		else if (menucode == 3) //종료
		{
			return 0;
		}
		system("cls");
	}
	return 0;
}


// ※game※

int keycontrol()
{
	char temp = getch();
	if (temp == '↑')
	{
		return UP;
	}
	else if (temp == '←')
	{
		return LEFT;
	}
	else if (temp == '↓')
	{
		return DOWN;
	}
	else if (temp == '→')
	{
		return RIGHT;
	}
	else if (temp == '\n')
	{
		return SELECT;
	}
}

//출력 함수
//처음 메뉴출력
int menudraw()
{
	int x = 55;
	int y = 25;
	gotoxy(x - 2, y);
	printf("> 게임시작");
	gotoxy(x, y + 1);
	printf("이어하기");
	gotoxy(x , y + 2);
	printf("게임설명");
	gotoxy(x + 2, y + 3);
	printf("죵료");
	while (1)
	{
		int n = keycontrol();
		switch (n)
		{
		case UP:
		{
			if (y > 25)
			{
				gotoxy(x - 2, y);
				printf(" ");
				gotoxy(x - 2, --y);
				printf(">");
			}
			break;
		}
		case DOWN:
		{
			if (y < 28)
			{
				gotoxy(x - 2, y);
				printf(" ");
				gotoxy(x - 2, ++y);
				printf(">");
			}
			break;
		}
		case SELECT:
		{
			return y - 25;
		}
		}
	}

}
//타이틀 출력
void titledraw()
{
	printf("\n");
	printf("\n");
	printf("                 $@.  ;@@@@@@@   \n");
	printf("                ,@*   *@~-----   \n");
	printf("                !@;   @$         \n");
	printf("               ,@@   :@,         \n");
	printf("               =@*   @#          \n");
	printf("              .@@,  ~@:          \n");
	printf("               $#.  ;@-          \n");
	printf("              .@=   @@$$$$=,     \n");
	printf("              !@,  -@@@@@@@!     \n");
	printf("              @@         !@,     \n");
	printf("             ~@:         @@      8 888888888o.               .8.          `8.`8888.      ,8'    d888888o.  \n");
	printf("             ;@~        :@-      8 8888    `^888.           .888.          `8.`8888.    ,8'   .`8888:' `88.\n");
	printf("             ##         =@.      8 8888        `88.        :88888.          `8.`8888.  ,8'    8.`8888.   Y8\n");
	printf("             @$   ##   .#=       8 8888         `88       . `88888.          `8.`8888.,8'     `8.`8888.\n");
	printf("            $@   !@,   ,@!       8 8888          88      .8. `88888.          `8.`88888'       `8.`8888.\n");
	printf("            @#   $@.   #@        8 8888          88     .8`8. `88888.          `8. 8888         `8.`8888.\n");
	printf("           ~@;  ,@*    @$        8 8888         ,88    .8' `8. `88888.          `8 8888          `8.`8888. \n");
	printf("           ;@-  ~@;   ;@-        8 8888    ,o88P'    .888888888. `88888.         8 8888      `8b.  ;8.`8888\n");
	printf("           @@   @@####@@,        8 8888    ,o88P'    .888888888. `88888.         8 8888      `8b.  ;8.`8888\n");
	printf("          -@;   *@@@@@#:         8 888888888P'      .8'       `8. `88888.        8 8888       `Y8888P ,88P'\n");
}
//게임설명 출력
void infodraw()
{
	system("cls");
	printf("■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■\n");
	printf("■                                                                                                                    ■\n");
	printf("■                                           <   게  임  설  명  >                                                    ■\n");
	printf("■                                                                                                                    ■\n");
	printf("■                                                                                                                    ■\n");
	printf("■                                                                                                                    ■\n");
	printf("■                                                                                                                    ■\n");
	printf("■                                                                                                                    ■\n");
	printf("■            1.   방향키를 이용하여 조작합니다. (  ↑ ,  ←  , ↓ , → , ENTER  )                                    ■\n");
	printf("■                                                                                                                    ■\n");
	printf("■            2.   택스트 선택 방식으로 진행되는 게임이며, 주어진 구조시간인 15일 동안 무사히 생존 하는것이           ■\n");
	printf("■                 목표입니다.                                                                                        ■\n");
	printf("■                                                                                                                    ■\n");
	printf("■            3.  생존자들은 총 3명이며 각각의 생존자들 마다 3의 HP를 가지고 있습니다.  식량 혹은 물을 섭취하지       ■\n");
	printf("■                하였을떄 HP가 감소하며 초반에 주어지는 구급상자를 통해 HP를 회복시킬 수 있습니다.                   ■\n");
	printf("■                (★모든 자원은 소모품입니다.)                                                                       ■\n");
	printf("■                                                                                                                    ■\n");
	printf("■            4.  탐색을 통해 자원을 획득하거나 잃을 수 있으며, 돌발 상황이 발생할수도 있습니다.                      ■\n");
	printf("■                                                                                                                    ■\n");
	printf("■            5.  그 날의 할 모든 일을 끝내면 반드시 다음날로 버튼을 선택해 주세요.                                   ■\n");
	printf("■                                                                                                                    ■\n");
	printf("■                                                                                                                    ■\n");
	printf("■                                                                                                                    ■\n");
	printf("■                                                                                                                    ■\n");
	printf("■                                                                                                                    ■\n");
	printf("■                                                                                                                    ■\n");
	printf("■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■\n");


	printf("\n\n\n");
	printf("뒤로 가려면 ENTER키를 눌러주시요.....\n");
	
	while (1)
	{
		
		if (keycontrol() == SELECT)
		{
			break;
		}
		else if (keycontrol() == SPACEBAR)
		{
			printf("\n\n");
			printf("팔라딘 오란에게 전해 head is dead end,now each numbers deny infomations now g ");
		}
	}
}
//물품 개수 출력
int obj_amount(struct object const *food,struct object const *water, struct object const *healpack,struct object const *gun,int date)
{
	int x = 2;
	int y = 1;
	gotoxy(x, y);
	setColor(yellow, black);
	if (food <= 0)
		food = 0;
	printf("FOOD : %d",food);
	gotoxy(x, y+1);
	setColor(lightblue, black);
	if (water <= 0)
		water = 0;
	printf("WATER : %d",water);
	gotoxy(x, y+2);
	setColor(purple, black);
	if (healpack <= 0)
		healpack = 0;
	printf("HEALPACK : %d", healpack);
	gotoxy(x, y + 3);
	setColor(brown, black);
	if (gun <= 0)
		gun = 0;
	printf("GUN : %d", gun);

	setColor(white, black);
	int X = 110;
	int Y = 1;
	if (date == 1)
	{
		gotoxy(X, Y);
		printf("2번째날");
	}
	else if (date == 2)
	{
		gotoxy(X, Y);
		printf("3번째날");
	}
	else if (date == 3)
	{
		gotoxy(X, Y);
		printf("4번째날");
	}
	else if (date == 4)
	{
		gotoxy(X, Y);
		printf("5번째날");
	}
	else if (date == 5)
	{
		gotoxy(X, Y);
		printf("6번째날");
	}
	else if (date == 6)
	{
		gotoxy(X, Y);
		printf("7번째날");
	}
	else if (date == 7)
	{
		gotoxy(X, Y);
		printf("8번째날");
	}
	else if (date == 8)
	{
		gotoxy(X, Y);
		printf("9번째날");
	}
	else if (date == 9)
	{
		gotoxy(X, Y);
		printf("10번째날");
	}
	else if (date == 10)
	{
		gotoxy(X, Y);
		printf("11번째날");
	}
	else if (date == 11)
	{
		gotoxy(X, Y);
		printf("12번째날");
	}
	else if (date == 12)
	{
		gotoxy(X, Y);
		printf("13번째날");
	}
	else if (date == 13)
	{
		gotoxy(X, Y);
		printf("14번째날");
	}
	else if (date == 14)
	{
		gotoxy(X, Y);
		printf("15번째날");
	}
}
