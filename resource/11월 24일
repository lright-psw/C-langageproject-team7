//캐릭터상태창 출력
void statedraw()
{
	int x = 2;
	int y = 24;

	//A의 상태창
	gotoxy(x, y);
	printf("어머니의 상태");
	if (hp.a_hp >= 3)
	{
		gotoxy(x, y + 1);
		setColor(red, black);
		printf("어머니의 체력  :  ■■■");
	}
	else if (hp.a_hp == 2)
	{
		gotoxy(x, y + 1);
		setColor(red, black);
		printf("어머니의 체력  :  ■■");
	}
	else if (hp.a_hp == 1)
	{
		gotoxy(x, y + 1);
		setColor(red, black);
		printf("어머니의 체력  :  ■");
	}
	else if (hp.a_hp <= 0)
	{
		gotoxy(x, y + 1);
		setColor(red, black);
		printf("어머니의 체력  :  ");
	}

	if (a_foodeat == 0)
	{
		gotoxy(x, y + 2);
		setColor(yellow, black);
		printf("어머니의 포만감 : ■■■");
	}
	else if (a_foodeat == 1)
	{
		gotoxy(x, y + 2);
		setColor(yellow, black);
		printf("어머니의 포만감 : ■■");
	}
	else if (a_foodeat == 2)
	{
		gotoxy(x, y + 2);
		setColor(yellow, black);
		printf("어머니의 포만감 : ■");
	}
	else if (a_foodeat >= 3)
	{
		gotoxy(x, y + 2);
		setColor(yellow, black);
		printf("어머니의 포만감 : ");
	}

	if (a_watereat == 0)
	{
		gotoxy(x, y + 3);
		setColor(lightblue, black);
		printf("어머니의 목마름 : ■■■");
	}
	else if (a_watereat == 1)
	{
		gotoxy(x, y + 3);
		setColor(lightblue, black);
		printf("어머니의 목마름 : ■■");
	}
	else if (a_watereat == 2)
	{
		gotoxy(x, y + 3);
		setColor(lightblue, black);
		printf("어머니의 목마름 : ■");
	}
	else if (a_watereat >= 3)
	{
		gotoxy(x, y + 3);
		setColor(lightblue, black);
		printf("어머니의 목마름 : ");
	}

	//B의 상태창
	gotoxy(x + 27, y);
	setColor(white, black);
	printf("아버지의 상태");
	if (hp.b_hp >= 3)
	{
		gotoxy(x + 27, y + 1);
		setColor(red, black);
		printf("아버지의 체력  :  ■■■");
	}
	else if (hp.b_hp == 2)
	{
		gotoxy(x + 27, y + 1);
		setColor(red, black);
		printf("아버지의 체력  :  ■■");
	}
	else if (hp.b_hp == 1)
	{
		gotoxy(x + 27, y + 1);
		setColor(red, black);
		printf("아버지의 체력  :  ■");
	}
	else if (hp.b_hp <= 0)
	{
		gotoxy(x+27, y + 1);
		setColor(red, black);
		printf("아버지의 체력  :  ");
	}

	if (b_foodeat == 0)
	{
		gotoxy(x + 27, y + 2);
		setColor(yellow, black);
		printf("아버지의 포만감 : ■■■");
	}
	else if (b_foodeat == 1)
	{
		gotoxy(x + 27, y + 2);
		setColor(yellow, black);
		printf("아버지의 포만감 : ■■");
	}
	else if (b_foodeat == 2)
	{
		gotoxy(x + 27, y + 2);
		setColor(yellow, black);
		printf("아버지의 포만감 : ■");
	}
	else if (b_foodeat >= 3)
	{
		gotoxy(x + 27, y + 2);
		setColor(yellow, black);
		printf("아버지의 포만감 : ");
	}

	if (b_watereat == 0)
	{
		gotoxy(x + 27, y + 3);
		setColor(lightblue, black);
		printf("아버지의 목마름 : ■■■");
	}
	else if (b_watereat == 1)
	{
		gotoxy(x + 27, y + 3);
		setColor(lightblue, black);
		printf("아버지의 목마름 : ■■");
	}
	else if (b_watereat == 2)
	{
		gotoxy(x + 27, y + 3);
		setColor(lightblue, black);
		printf("아버지의 목마름 : ■");
	}
	else if (b_watereat >= 3)
	{
		gotoxy(x + 27, y + 3);
		setColor(lightblue, black);
		printf("아버지의 목마름 : ");
	}


	//C의 상태창
	gotoxy(x + 54, y);
	setColor(white, black);
	printf("아이의 상태");
