	{
		gotoxy(x + 54, y + 1);
		setColor(red, black);
		printf("아이의 체력  :  ■■");
	}
	else if (hp.c_hp == 1)
	{
		gotoxy(x + 54, y + 1);
		setColor(red, black);
		printf("아이의 체력  :  ■");
	}
	else if (hp.c_hp <= 0)
	{
		gotoxy(x+54, y + 1);
		setColor(red, black);
		printf("아이의 체력  :  ");
	}

	if (c_foodeat == 0)
	{
		gotoxy(x + 54, y + 2);
		setColor(yellow, black);
		printf("아이의 포만감 : ■■■");
	}
	else if (c_foodeat == 1)
	{
		gotoxy(x + 54, y + 2);
		setColor(yellow, black);
		printf("아이의 포만감 : ■■");
	}
	else if (c_foodeat == 2)
	{
		gotoxy(x + 54, y + 2);
		setColor(yellow, black);
		printf("아이의 포만감 : ■");
	}
	else if (c_foodeat >= 3)
	{
		gotoxy(x + 54, y + 2);
		setColor(yellow, black);
		printf("아이의 포만감 : ");
	}

	if (c_watereat == 0)
	{
		gotoxy(x + 54, y + 3);
		setColor(lightblue, black);
		printf("아이의 목마름 : ■■■");
	}
	else if (c_watereat == 1)
	{
		gotoxy(x + 54, y + 3);
		setColor(lightblue, black);
		printf("아이의 목마름 : ■■");
	}
	else if (c_watereat == 2)
	{
		gotoxy(x + 54, y + 3);
